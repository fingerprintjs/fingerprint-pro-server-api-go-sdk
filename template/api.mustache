            path := "{{{path}}}"

            for i, arg := range args {
                path = strings.Replace(path, "{"+pathParams[i]+"}", arg, -1)
            }

            return path
        },
        StatusCodeResultsFactoryMap: map[int]func() any{
            {{#responses}}{{#dataType}}
            {{code}}: func() any { return &{{{dataType}}}{} },
            {{/dataType}}{{/responses}}
        },
